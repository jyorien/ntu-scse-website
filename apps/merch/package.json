{
  "name": "merch",
  "description": "NTU SCSE Merch Backend",
  "version": "0.1.0",
  "main": "dist/server.js",
  "source": "src/server.ts",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsoa spec-and-routes && tsup src/index.ts --format cjs",
    "start": "node dist/index.js",
    "clean": "rm -rf .turbo && rm -rf dist",
    "dev": "concurrently -n TSUP,TSOA -c grey,green \"yarn run dev:tsup\" \"yarn run dev:tsoa\"",
    "dev:tsup": "tsup src/index.ts --format cjs --watch --onSuccess \"node dist/index.js\"",
    "dev:tsoa": "onchange -i 'src/**/*' -- yarn run tsoa spec-and-routes",
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "lint:fix": "TIMING=1 eslint --fix \"**/*.ts*\""
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.289.0",
    "@aws-sdk/util-dynamodb": "^3.289.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nodelogger": "*",
    "stripe": "^12.5.0",
    "swagger-ui-express": "^4.6.3",
    "tsoa": "^5.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "concurrently": "^8.2.0",
    "cookie-parser": "^1.4.6",
    "onchange": "^7.1.0",
    "tsconfig": "*",
    "tsup": "^6.7.0",
    "types": "*",
    "typescript": "^4.8.4"
  }
}
